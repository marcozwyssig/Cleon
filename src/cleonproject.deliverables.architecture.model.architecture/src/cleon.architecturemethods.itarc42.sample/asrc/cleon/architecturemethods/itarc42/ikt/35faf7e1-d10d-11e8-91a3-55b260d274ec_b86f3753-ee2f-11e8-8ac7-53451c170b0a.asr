        </predicate>
        <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
          <object statement-guid="2b46ef02-ee30-11e8-8ac7-53451c170b0a" type="literal">Zusätzliche Komplexität im Aufbau und im Betrieb (Unterhalt)</object>
        </predicate>
