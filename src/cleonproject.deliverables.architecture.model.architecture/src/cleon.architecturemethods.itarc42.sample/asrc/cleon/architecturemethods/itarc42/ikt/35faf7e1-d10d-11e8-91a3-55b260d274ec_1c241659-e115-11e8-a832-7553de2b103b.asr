  <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
    <object statement-guid="ffc6401d-02cb-11e9-9e58-33d596257b14" type="resource" guid="bf167f4d-02cb-11e9-9e58-33d596257b14" />
