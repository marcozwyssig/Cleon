        </predicate>
        <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="861fe3e9-0437-11e9-bc2b-2f2cf95d8322" type="literal">IKT Zentrale</object>
        </predicate>
        <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
          <object statement-guid="bb670012-09b9-11e9-be8a-5d6c64a17144" type="literal">Der zentrale IKT-Betreiber (2nd und 3rd Level Support) ist für den Betrieb des NMS und der Bundeskomponenten in den TNs verantwortlich. </object>
          <object statement-guid="d8ac762e-1d9e-11e9-b601-db75aa6f89a4" type="literal">Zum aktuellen Zeitpunkt ist die Firma Atos AG für diese Rolle verantwortlich.</object>
        </predicate>
        <predicate guid="a3e89849-b53d-11e5-bc0c-f35b68c3609a">
          <object statement-guid="51ffe5b0-9c12-11e9-9470-2178b598e9a4" type="literal">3</object>
        </predicate>
        <predicate guid="9c0f8e0e-0523-11e9-95f7-3b0c47908608">
          <object statement-guid="50d5f405-09da-11e9-8a72-9b290e83b8f2">
            <resource guid="50d5ccf1-09da-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="50d5f403-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="54496ff8-09da-11e9-8a72-9b290e83b8f2" type="literal">operates</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="e2113c84-0ccd-11e9-83ba-4d57b06560a2" type="literal">Der IKT-Betreiber verwendet für den Betrieb die jeweiligen Management-Werkzeuge.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="50d5f404-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="3c252b07-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
          <object statement-guid="b5365d52-09da-11e9-8a72-9b290e83b8f2">
            <resource guid="b536363e-09da-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="b5363640-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="566a5dd9-09db-11e9-8a72-9b290e83b8f2" type="literal">monitors</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="0f656478-0cce-11e9-83ba-4d57b06560a2" type="literal">Der IKT-Betreiber verwendet für den Betrieb die jeweilige Systemüberwachung.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="b5363641-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="73e2e196-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
          <object statement-guid="0c3ad2b0-09db-11e9-8a72-9b290e83b8f2">
            <resource guid="0c3aab9c-09db-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="0c3ad2ae-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="12dcab2a-09db-11e9-8a72-9b290e83b8f2" type="literal">makeChanges</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="2654d2b7-0cce-11e9-83ba-4d57b06560a2" type="literal">Der IKT-Betreiber spielt Änderungen am System über das Configuration Management ein, womit ein automatisches Provisionieren (Konfigurationen und Artefakte, Pakete) von Konfigurationen gewährleistet wird.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="0c3ad2af-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="eafe5e01-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
          <object statement-guid="158e83e2-09db-11e9-8a72-9b290e83b8f2">
            <resource guid="158e5dce-09db-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="158e5dd0-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="16b21c5c-09db-11e9-8a72-9b290e83b8f2" type="literal">adding</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="61bfd970-0cce-11e9-83ba-4d57b06560a2" type="literal">Der IKT-Betreiber fügt neue Artefakte und Pakete in das Repository hinzu, welches vom Configuration Management verwendet werden kann.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="158e5dd1-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="faa24645-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
        </predicate>
      </resource>
    </object>
    <object statement-guid="87fe67ed-0437-11e9-bc2b-2f2cf95d8322">
      <resource guid="87fe40da-0437-11e9-bc2b-2f2cf95d8322">
        <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="87fe67ec-0437-11e9-bc2b-2f2cf95d8322" type="resource" guid="a037dbde-2660-11e6-ae54-e50d44645ae7" />
        </predicate>
        <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="9fb4cf68-0437-11e9-bc2b-2f2cf95d8322" type="literal">IKT TN-Betreiber</object>
        </predicate>
        <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
          <object statement-guid="c12e570e-09b9-11e9-be8a-5d6c64a17144" type="literal">Der IKT TN-Betreiber ist für den Betrieb der kantonalen Komponenten zuständig. Der IKT Netzbetreiber hat prinzipiell Lesezugriff auf die Bundeskomponenten, damit bei Störungen eine wirtschaftliche Fehlerdiagnose sichergestellt wird.</object>
        </predicate>
        <predicate guid="a3e89849-b53d-11e5-bc0c-f35b68c3609a">
          <object statement-guid="520081f5-9c12-11e9-9470-2178b598e9a4" type="literal">8</object>
        </predicate>
        <predicate guid="9c0f8e0e-0523-11e9-95f7-3b0c47908608">
          <object statement-guid="65c0dee4-09da-11e9-8a72-9b290e83b8f2">
            <resource guid="65c0b7d0-09da-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="65c0dee2-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="66c72b5d-09da-11e9-8a72-9b290e83b8f2" type="literal">operates</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="af6cf41d-0cce-11e9-83ba-4d57b06560a2" type="literal">Der IKT TN-Betreiber kann die Benutzer für den Zugriff auf das System verwalten (z.B. neue Benutzer erstellen, ändern oder entfernen).</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="65c0dee3-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="3c252b07-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
          <object statement-guid="dbd342aa-1344-11e9-827f-fdc23731fac9">
            <resource guid="dbd194f6-1344-11e9-827f-fdc23731fac9">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="dbd1e318-1344-11e9-827f-fdc23731fac9" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="dd80cba4-1344-11e9-827f-fdc23731fac9" type="literal">monitors</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="def9d738-1344-11e9-827f-fdc23731fac9" type="literal">Der IKT TN-Betreiber kann bei Störungen auf die Systemüberwachung zugreifen, um allenfalls Erkentnnise für die Behebung zu gewinnen.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="dbd2f489-1344-11e9-827f-fdc23731fac9" type="resource" guid="73e2e196-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
        </predicate>
      </resource>
    </object>
    <object statement-guid="03487356-0438-11e9-bc2b-2f2cf95d8322">
      <resource guid="03484c43-0438-11e9-bc2b-2f2cf95d8322">
        <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="03484c45-0438-11e9-bc2b-2f2cf95d8322" type="resource" guid="a037dbde-2660-11e6-ae54-e50d44645ae7" />
        </predicate>
        <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="04d26eb7-0438-11e9-bc2b-2f2cf95d8322" type="literal">System-Integrator</object>
        </predicate>
        <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
          <object statement-guid="c6460d17-09b9-11e9-be8a-5d6c64a17144" type="literal">Der System-Integrator ist für die Erweiterung, Integration und Entwicklung von Konfigurationen und Komponenten verantwortlich.</object>
          <object statement-guid="90b72f85-25ea-11e9-b1c9-75826cd6daa5" type="literal">Zum aktuellen Zeitpunkt ist die Firma Atos AG für diese Rolle verantwortlich.</object>
        </predicate>
        <predicate guid="a3e89849-b53d-11e5-bc0c-f35b68c3609a">
          <object statement-guid="52005ae4-9c12-11e9-9470-2178b598e9a4" type="literal">7</object>
        </predicate>
        <predicate guid="9c0f8e0e-0523-11e9-95f7-3b0c47908608">
          <object statement-guid="1d2ac250-09db-11e9-8a72-9b290e83b8f2">
            <resource guid="1d2a9b3c-09db-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="1d2ac24e-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="1e5a8fc9-09db-11e9-8a72-9b290e83b8f2" type="literal">develops</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="ce305b9c-0ccf-11e9-83ba-4d57b06560a2" type="literal">Die Entwicklung entwickelt die entsprechenden deklarativen Konfigurationen und stellt sie im Configuration Management zur Verfügung.</object>
                <object statement-guid="f17a5307-0ccf-11e9-83ba-4d57b06560a2" type="literal">Die Verteilung der Konfigurationen liegt nicht in der Verantwortung der Entwicklung.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="1d2ac24f-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="eafe5e01-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
          <object statement-guid="20282401-09db-11e9-8a72-9b290e83b8f2">
            <resource guid="2027fced-09db-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="2027fcef-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="20d553fa-09db-11e9-8a72-9b290e83b8f2" type="literal">adding</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="01fd7808-0cd0-11e9-83ba-4d57b06560a2" type="literal">Die Entwicklung stellt die neuen Artefakte und Pakete für die Verteilung zur Verfügung.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="20282400-09db-11e9-8a72-9b290e83b8f2" type="resource" guid="faa24645-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
        </predicate>
      </resource>
    </object>
    <object statement-guid="5d9499ac-0438-11e9-bc2b-2f2cf95d8322">
      <resource guid="5d947299-0438-11e9-bc2b-2f2cf95d8322">
        <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="5d94729b-0438-11e9-bc2b-2f2cf95d8322" type="resource" guid="a037dbde-2660-11e6-ae54-e50d44645ae7" />
        </predicate>
        <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="d283c006-0438-11e9-bc2b-2f2cf95d8322" type="literal">Service Desk</object>
        </predicate>
        <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
          <object statement-guid="c99908b0-09b9-11e9-be8a-5d6c64a17144" type="literal">Aufbau und Betrieb einer Support-Organisation auf dem Support-Level 1 (1st Level Support), welche einen 24/7/365 Bereitschaftsdienst bietet. Der Support wird durch einen Service Desk abgedeckt (mit Namen "Service Desk IP2.0").</object>
          <object statement-guid="8d92bda5-25ea-11e9-b1c9-75826cd6daa5" type="literal">Zum aktuellen Zeitpunkt ist die Firma Atos AG für diese Rolle verantwortlich.</object>
        </predicate>
        <predicate guid="a3e89849-b53d-11e5-bc0c-f35b68c3609a">
          <object statement-guid="51ffe5b1-9c12-11e9-9470-2178b598e9a4" type="literal">4</object>
        </predicate>
        <predicate guid="9c0f8e0e-0523-11e9-95f7-3b0c47908608">
          <object statement-guid="c95ac3dd-09da-11e9-8a72-9b290e83b8f2">
            <resource guid="c959b269-09da-11e9-8a72-9b290e83b8f2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="c959b26a-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="c959b26c-09da-11e9-8a72-9b290e83b8f2" type="literal">monitors</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="154b7bae-0cd0-11e9-83ba-4d57b06560a2" type="literal">Der Service-Desk prüft mit Hilfe der Systemüberwachung den Systemzustand und eskaliert bei Störungen den IKT-Betreiber und/oder TN-Betreiber.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="d1709ecc-09da-11e9-8a72-9b290e83b8f2" type="resource" guid="73e2e196-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
        </predicate>
      </resource>
    </object>
    <object statement-guid="fc8595ba-0cce-11e9-83ba-4d57b06560a2">
      <resource guid="fc856ea7-0cce-11e9-83ba-4d57b06560a2">
        <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="fc8595b9-0cce-11e9-83ba-4d57b06560a2" type="resource" guid="a037dbde-2660-11e6-ae54-e50d44645ae7" />
        </predicate>
        <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="fe20a81e-0cce-11e9-83ba-4d57b06560a2" type="literal">Sicherheitsverantwortlicher</object>
        </predicate>
        <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
          <object statement-guid="e05fb7d4-249f-11e9-a370-bdb19dc5ffd6" type="literal">Zuständig und verantwortlich für die Sicherheit. Alle sicherheitsrelevanten Themen, z.B. Autorisierung für Zugriffe usw., müssen durch den Sicherheitsverantwortlichen beurteilt und genehmigt werden.</object>
          <object statement-guid="05491606-0ccf-11e9-83ba-4d57b06560a2" type="literal">Er kann Einsicht in die Protokolle zur Nachvollziehbarkeit (Nichtabstreitbarkeit, Auditing) verlangen und bei kritischen Komponenten agiert er im Vier-Augen Prinzip (Segregation of Duty).</object>
          <object statement-guid="9b87bd1a-25ea-11e9-b1c9-75826cd6daa5" type="literal">Das BABS ist für diese Rolle verantwortlich.</object>
        </predicate>
        <predicate guid="a3e89849-b53d-11e5-bc0c-f35b68c3609a">
          <object statement-guid="52000cc2-9c12-11e9-9470-2178b598e9a4" type="literal">5</object>
        </predicate>
        <predicate guid="9c0f8e0e-0523-11e9-95f7-3b0c47908608">
          <object statement-guid="53879a23-0ccf-11e9-83ba-4d57b06560a2">
            <resource guid="53879a1f-0ccf-11e9-83ba-4d57b06560a2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="53879a21-0ccf-11e9-83ba-4d57b06560a2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="563e079c-0ccf-11e9-83ba-4d57b06560a2" type="literal">secures</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="5bb14b6d-0ccf-11e9-83ba-4d57b06560a2" type="literal">Bei kritischen Komponenten muss das Einspielen und das Verteilen von Konfigurationen von unterschiedlichen Rollen (Aktoren) durchgeführt werden.</object>
                <object statement-guid="834918f4-0ccf-11e9-83ba-4d57b06560a2" type="literal">Der Sicherheitsverantwortliche prüft und verteilt die entsprechenden Konfigurationen.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="53879a22-0ccf-11e9-83ba-4d57b06560a2" type="resource" guid="3c252b07-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
          <object statement-guid="984ae7e2-0ccf-11e9-83ba-4d57b06560a2">
            <resource guid="984a99be-0ccf-11e9-83ba-4d57b06560a2">
              <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="984ac0d0-0ccf-11e9-83ba-4d57b06560a2" type="resource" guid="8ac0190f-0523-11e9-95f7-3b0c47908608" />
              </predicate>
              <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
                <object statement-guid="a35a2c9b-0ccf-11e9-83ba-4d57b06560a2" type="literal">verifies</object>
              </predicate>
              <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
                <object statement-guid="ab29d65f-0ccf-11e9-83ba-4d57b06560a2" type="literal">Der Sicherheitsverantwortliche prüft die Auditing Protokolle auf Mängel und/oder Vorfälle der Sicherheit.</object>
              </predicate>
              <predicate guid="ba5c0df0-0523-11e9-95f7-3b0c47908608">
                <object statement-guid="984ac0d1-0ccf-11e9-83ba-4d57b06560a2" type="resource" guid="73e2e196-09da-11e9-8a72-9b290e83b8f2" />
              </predicate>
            </resource>
          </object>
        </predicate>
      </resource>
    </object>
    <object statement-guid="c9322f1e-249f-11e9-a370-bdb19dc5ffd6">
      <resource guid="c932080b-249f-11e9-a370-bdb19dc5ffd6">
        <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="c932080d-249f-11e9-a370-bdb19dc5ffd6" type="resource" guid="a037dbde-2660-11e6-ae54-e50d44645ae7" />
        </predicate>
        <predicate guid="33fc8660-f91f-11d9-bb45-5fabdff7c7da">
          <object statement-guid="c9d414aa-249f-11e9-a370-bdb19dc5ffd6" type="literal">Betriebsmanagement</object>
        </predicate>
        <predicate guid="d63ca21b-a7e7-11e5-82dd-3b995d9c840c">
          <object statement-guid="d617e1ca-249f-11e9-a370-bdb19dc5ffd6" type="literal">Der Bund ist der Projektsponsor und die Schnittstelle zu den kantonalen und zu anderen nationalen Organisationen.</object>
          <object statement-guid="a78ab0dd-25ea-11e9-b1c9-75826cd6daa5" type="literal">Das BABS ist für diese Rolle verantwortlich.</object>
        </predicate>
        <predicate guid="a3e89849-b53d-11e5-bc0c-f35b68c3609a">
          <object statement-guid="51ffbe9f-9c12-11e9-9470-2178b598e9a4" type="literal">2</object>
        </predicate>
      </resource>
    </object>
  </predicate>
