  <predicate guid="3169f49a-f91f-11d9-bb45-5fabdff7c7da">
    <object statement-guid="09a2aa3d-02cc-11e9-9e58-33d596257b14" type="resource" guid="ca8bd8df-02cb-11e9-9e58-33d596257b14" />
